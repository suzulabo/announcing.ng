{
  "name": "announcing-cloudflare",
  "private": true,
  "scripts": {
    "dev": "wrangler dev --local",
    "start": "wrangler dev",
    "build": "tsc && wrangler publish --tsconfig tsconfig.build.json --dry-run --outdir dist && du -h dist/main.js",
    "deploy": "tsc && wrangler publish --tsconfig tsconfig.build.json --minify",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "lint": "eslint src && prettier --check src"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.14.1",
    "jest-environment-miniflare": "^2.7.1",
    "wrangler": "^2.0.27"
  },
  "dependencies": {
    "@suzulabo/path-matcher": "^0.0.0",
    "jose": "^4.9.2"
  }
}
